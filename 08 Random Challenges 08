import random as r

score = 0

for i in range(5):
    num1 = r.randint(0, 10)
    num2 = r.randint(0, 10)

    answer = int(input("What is "+ str(num1) + " " +"+"+ " "+ str(num2) + "?"))
    if answer == num1  + num2:
        print("Correct!")
        score += 1
    else:
        print("You're wrong.")

print("You got", score, "correct.")
