def timestable():
    num = int(input("Enter a number between 1 and 12. "))
    for i in range(1, 13):
        print(num * i)

print("0 - Exit")
print("1 - Timetables")
ans = int(input())

if ans == 0:
    print("Bye!")
elif ans == 1:
    timestable()
else:
    print("Please enter a valid choice.")
