num = int(input("How many people would you like to invite to the party? "))

if num > 9:
    print("Too many people!")

else:
    for i in range(1, num+1):
        name = input("What is the name of person number " + str(i) + "? ")
        print("Person number" , str(i) + ", " + name + ", has been invited to the party.")
        
